<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".signUpPage.SignPageFragment">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="46dp"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="74dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/register_txt"
            app:srcCompat="@drawable/arrow" />

        <TextView
            android:id="@+id/register_txt"
            style="@style/TextAppearance.MaterialComponents.Caption"
            android:layout_width="317dp"
            android:layout_height="52dp"
            android:layout_marginStart="43dp"
            android:layout_marginTop="43dp"
            android:text="@string/no_account"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/password_toggle" />

        <TextView
            android:id="@+id/login_btn"
            android:layout_width="77dp"
            android:layout_height="33dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/advent_pro_semibold"
            android:text="@string/log_in"
            android:textAlignment="center"
            android:textColor="#D8C5C5"
            android:textSize="24sp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/register_txt" />

        <EditText
            android:id="@+id/emil_txt"
            android:layout_width="334dp"
            android:layout_height="71dp"

            android:layout_marginTop="177dp"
            android:layout_marginEnd="41dp"
            android:background="@drawable/text_filed"
            android:hint="@string/email"
            android:inputType="textEmailAddress"
            android:padding="10sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/password_toggle"
            android:layout_width="336dp"
            android:layout_height="69dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="37dp"
            android:background="@drawable/text_filed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/emil_txt"
            app:passwordToggleEnabled="true">

            <EditText
                android:id="@+id/password_txt"
                android:layout_width="338dp"
                android:layout_height="71dp"
                android:background="@drawable/text_filed"
                android:hint="@string/password"
                android:inputType="textPassword"
                tools:ignore="SpeakableTextPresentCheck" />

        </com.google.android.material.textfield.TextInputLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>